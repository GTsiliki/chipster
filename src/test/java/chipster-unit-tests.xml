<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd">

<suite name="Chipster unit test suite" configfailurepolicy="continue">
  <!-- without configfailurepolicy TestNG is too eager to skip unrelated tests when initialisers fails somewhere else -->
  
  <test name="Unit test">
  	<groups>
		<run>
			<include name="unit" />
		</run>
	</groups>
  
  
  <packages>
  	<package name="fi.csc.microarray.*"></package>
  </packages>

  </test>
</suite>